		<queryString>
			<![CDATA[SELECT SUM(CASE WHEN tft.DEBIT THEN tft.LOCAL_AMOUNT * -1 ELSE tft.LOCAL_AMOUNT END )
TAKEOVER
FROM TFN_FINANCE_TXN as tft
WHERE tft.TXNDATE < $P{START_DATE_FOR_TAKEOVER} 
AND tft.ACCOUNT_ID = $P{ACCOUNT_ID}]]>
