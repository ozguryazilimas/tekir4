<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="fatura">
    <xs:complexType>
      <xs:sequence>
        <!--UBL KARSILIGI YOK - CS-XML'in versionunu iceriyor-->
        <xs:element name="xsdVersion" type="xs:string" minOccurs="0" />
        <!--cbc:UBLVerisonID - Giden faturada boş gönderilir ise 2.1 değeri ile UBL oluşturulur, dolu gonderilir ise buradaki değeri ile UBL oluşturulur kullanılmaz, gelen faturanın UBL versionunu belirtir. örn: 2.0, 2.1-->
        <xs:element name="ublVersionNumarasi" type="xs:string" minOccurs="0" />
        <!--cbc:CustomizationID - Giden faturada boş gönderilir ise TR1.2 değeri ile UBL oluşturulur, dolu gonderilir ise buradaki değeri ile UBL oluşturulur, faturanın özelleştirme numarasını belirtir. örn: TR1.0, TR1.2-->
        <xs:element name="ublOzellestirmeNumarasi" type="xs:string" minOccurs="0" />
        <!-- UBL KARSILIGI YOK - giden fatura için tercih edilen faturaNo ön eki. A veya B veya C ile başlamayan 2 karakter. Orn: RR girildiüinde RRA2014000000001 gibi bir faturano oluşacaktır -->
        <xs:element name="tercihEdilenSeriNo" type="xs:string" minOccurs="0" />
        <!-- cbc:ProfileID - GIB fatura senaryosu, temel fatura veya ticari fatura [TEMELFATURA, TICARIFATURA]-->
        <xs:element name="faturaTuru" type="xs:string" />
        <!--cbc:ID - giden fatura icin yerel fatura no, gelen fatura icin fatura no-->
        <xs:element name="faturaNo" type="xs:string" minOccurs="0" />
        <!--cbc:CopyIndicator - Asıl Suret bilgisi, true olarak doldurulur ise "suret" anlamina gelir, default değeri false'dir-->
        <xs:element name="suret" type="xs:boolean" minOccurs="0" />
        <!-- cbc:UUID - giden fatura için boş olmalı, gelen fatura için ETTN (uuid)-->
        <xs:element name="faturaId" type="xs:string" minOccurs="0" />
        <!-- cbc:IssueDate - invoice issue date format=yyyy-mm-dd -->
        <xs:element name="faturaTarihi" type="tarihTipi" />
        <!-- cbc:IssueTime -  invoice issue time format=hh:mm:ss-->
        <xs:element name="faturaZamani" type="zamanTipi" minOccurs="0" />
        <!-- cbc:InvoiceTypeCode - invoice type code determined by revenue adminstration [SATIS, ISTISNA, OZELMATRAH, ARACTESCIL, TEVKIFAT, IADE]-->
        <xs:element name="faturaTipi" type="xs:string" />
        <!--  cbc:Note - note -->
        <xs:element name="faturaNot" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
        <!-- cbc:DocumentCurrencyCode - currency code -->
        <xs:element name="paraBirimi" type="xs:string" />
        <!-- cbc:TaxCurrencyCode -->
        <xs:element name="vergiParaBirimi" type="xs:string" minOccurs="0" />
        <!--cbc:PricingCurrencyCode-->
        <xs:element name="fiyatlandirmaParaBirimi" type="xs:string" minOccurs="0" />
        <!--cbc:PaymentCurrencyCode-->
        <xs:element name="odemeParaBirimi" type="xs:string" minOccurs="0" />
        <!-- cbc:PaymentAlternativeCurrencyCode -->
        <xs:element name="alternatifOdemeParaBirimi" type="xs:string" minOccurs="0" />
        <!--cbc:AccountingCost - Alt fatura Tipi-->
        <xs:element name="altFaturaTipi" type="xs:string" minOccurs="0" />
        <!--cac:InvoicePeriod-->
        <xs:element name="faturaDonemi" type="donemTipi" minOccurs="0" />
        <!--cac:OrderReference-->
        <xs:element name="siparisBilgileri" type="siparisBilgileriTipi" minOccurs="0" />
        <!--cac:BillingReference-->
        <xs:element name="faturaReferansBilgileri" type="faturaReferansBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:DespatchDocumentReference-->
        <xs:element name="irsaliyeBilgileri" type="dokumanBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:ReceiptDocumentReference-->
        <xs:element name="malKabulBilgileri" type="dokumanBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:OriginatorDocumentReference-->
        <xs:element name="baslangicDokumanBilgileri" type="dokumanBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:ContractDocumentReference-->
        <xs:element name="kontratDokumanBilgileri" type="dokumanBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:AdditionalDocumentReference-->
        <xs:element name="ilaveDokuman" type="dokumanBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:AccountingSupplierParty-->
        <xs:element name="satici" type="tarafBilgileriTipi" />
        <!--cac:AccountingCustomerParty-->
        <xs:element name="alici" type="tarafBilgileriTipi" />
        <!--cac:BuyerCustomerParty-->
        <xs:element name="hizmetiAlan" type="tarafBilgileriTipi" minOccurs="0" />
        <!--cac:SellerSupplierParty-->
        <xs:element name="hizmetiSaglayan" type="tarafBilgileriTipi" minOccurs="0" />
        <!--cac:TaxRepresentativeParty-->
        <xs:element name="vergiSorumlusu" type="tarafBilgileriTipi" minOccurs="0" />
        <!--cac:Delivery-->
        <xs:element name="gonderimSevkiyatTasimaBilgileri" type="gonderimSevkiyatTasimaBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:PaymentMeans-->
        <xs:element name="odemeSekli" type="odemeSekliTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:PaymentTerms-->
        <xs:element name="odemeKosullari" type="odemeKosulTipi" minOccurs="0" />
        <!--cac:AllowanceCharge-->
        <xs:element name="iskontoArtirim" type="iskontoArtirimTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--TaxExchangeRate - cac:ExchangeRate-->
        <xs:element name="vergiDovizKuru" type="dovizKuruTipi" minOccurs="0" />
        <!--PricingExchangeRate - cac:ExchangeRate-->
        <xs:element name="fiyatlandirmaDovizKuru" type="dovizKuruTipi" minOccurs="0" />
        <!--PaymentExchangeRate - cac:ExchangeRate-->
        <xs:element name="odemeDovizKuru" type="dovizKuruTipi" minOccurs="0" />
        <!--PaymentAlternativeExchangeRate - cac:ExchangeRate-->
        <xs:element name="alternatifOdemeDovizKuru" type="dovizKuruTipi" minOccurs="0" />
        <!--cac:TaxTotal-->
        <xs:element name="vergiler" type="vergilerTipi" maxOccurs="unbounded" />
        <!--cbc:WithholingtaxTotal-->
        <xs:element name="tevkifatlar" type="vergilerTipi" minOccurs="0" maxOccurs="unbounded" />
        <!--cac:LegalMonateryTotal-->
        <xs:element name="parasalToplamlar" type="parasalToplamlarTipi" />
        <!--cac:InvoiceLine-->
        <xs:element name="faturaSatir" type="faturaSatirTipi" maxOccurs="unbounded" />
        <!--UBL KARSILIGI YOK - Ek bilgileri içeriyor-->
        <xs:element name="ekBilgiler" type="ekBilgi" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--DocumentReferenceType-->
  <xs:complexType name="dokumanBilgileriTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="belgeNo" type="idTipi" />
      <!--cbc:IssueDate-->
      <xs:element name="belgeTarihi" type="tarihTipi" />
      <!--cbc:DocumentTypeCode-->
      <xs:element name="belgeTuruKodu" type="xs:string" minOccurs="0" />
      <!--cbc:DocumentType-->
      <xs:element name="belgeTuru" type="xs:string" minOccurs="0" />
      <!--cbc:Documentdescription-->
      <xs:element name="dokumanAciklamasi" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
      <!--cbc:attachment-->
      <xs:element name="ekliDosya" type="ekliDosyaTipi" minOccurs="0" />
      <!--cac:ValidityPeriod-->
      <xs:element name="gecerlilikDonemi" type="donemTipi" minOccurs="0" />
      <!--cac:IssuerParty-->
      <xs:element name="ilgiliTarafBilgileri" type="tarafBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--LineReferenceType-->
  <xs:complexType name="satirReferansiTipi">
    <xs:sequence>
      <!--cbc:LineID-->
      <xs:element name="kalemNumarasi" type="idTipi" />
      <!--cbc:LineStatusCode-->
      <xs:element name="kalemDurumu" type="xs:string" minOccurs="0" />
      <!--cac:DocumentReference-->
      <xs:element name="belgeBilgileri" type="dokumanBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--PartyType-->
  <xs:complexType name="tarafBilgileriTipi">
    <xs:sequence>
      <!--cbc:WebSiteURI-->
      <xs:element name="webSitesi" type="xs:string" minOccurs="0" />
      <!--cbc:IndustryClassificationCode-->
      <xs:element name="naceKodu" type="xs:string" minOccurs="0" />
      <!--cac:PartyIdentification-->
      <xs:element name="aliciSaticiTanimi" type="idTipi" maxOccurs="unbounded" />
      <!--cac:PartyName-->
      <xs:element name="unvan" type="xs:string" minOccurs="0" />
      <!--cac:PostalAddress-->
      <xs:element name="postaAdresi" type="adresTipi" />
      <!--cac:PartyTaxScheme-->
      <xs:element name="vergiDairesi" type="xs:string" minOccurs="0" />
      <!--cac:PartyLegalEntity-->
      <xs:element name="sicilBilgileri" type="sicilBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
      <!--cac:Contact-->
      <xs:element name="irtibatBilgileri" type="irtibatBilgileriTipi" minOccurs="0" />
      <!--cac:Person-->
      <xs:element name="kisibilgileri" type="kisiBilgileriTipi" minOccurs="0" />
      <!--cac:AgentParty-->
      <xs:element name="subeBilgileri" type="tarafBilgileriTipi" minOccurs="0" />
      <!-- UBL'de INVOICE ICINDE KARSILIGI YOK sender/receiver identifier -->
      <xs:element name="etiket" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <!--OrderReferenceType-->
  <xs:complexType name="siparisBilgileriTipi">
    <xs:sequence>
      <!--cbc:Id-->
      <xs:element name="siparisNo" type="idTipi" />
      <!--cbc:SalesOrderID-->
      <xs:element name="saticiSiparisNo" type="idTipi" minOccurs="0" />
      <!--cbc:IssueDate-->
      <xs:element name="siparisTarihi" type="tarihTipi" />
      <!--cbc:OrderTypeCode-->
      <xs:element name="siparisTipi" type="xs:string" minOccurs="0" />
      <!--cbc:DocumentReference-->
      <xs:element name="siparisDokumani" type="dokumanBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--OrderLineReferenceType-->
  <xs:complexType name="siparisKalemiReferansiTipi">
    <xs:sequence>
      <!--cbc:LineID-->
      <xs:element name="kalemNumarasi" type="idTipi" />
      <!--cbc:SalesOrderLineID-->
      <xs:element name="saticiKalemNumarasi" type="idTipi" minOccurs="0" />
      <!--cbc:UUID-->
      <xs:element name="ETTN" type="xs:string" minOccurs="0" />
      <!--cbc:LineStatusCode-->
      <xs:element name="kalemDurumu" type="xs:string" minOccurs="0" />
      <!--cac:OrderReference-->
      <xs:element name="siparisBelgesi" type="siparisBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--BillingReferenceType-->
  <xs:complexType name="faturaReferansBilgileriTipi">
    <xs:sequence>
      <!--cac:InvoiceDocumentReference-->
      <xs:element name="ilgiliFatura" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:SelfBilledInvoiceDocumentReference-->
      <xs:element name="kendineFatura" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:CreditNoteDocumentReference-->
      <xs:element name="alacakDekontu" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:SelfBilledCreditNoteDocumentReference-->
      <xs:element name="kendineAlacakDekontu" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:DebitNoteDocumentReference-->
      <xs:element name="borcDekontu" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:ReminderDocumentReference-->
      <xs:element name="hatirlatmaBelgesi" type="dokumanBilgileriTipi" minOccurs="0" />
      <!--cac:AdditionalDocumentReference-->
      <xs:element name="KalemReferansi" type="faturaKalemReferansBilgileriTipi" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <!--BillingReferenceLineType-->
  <xs:complexType name="faturaKalemReferansBilgileriTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="kalemNo" type="idTipi" />
      <!--cbc:Amount-->
      <xs:element name="tutar" type="tutarTipi" minOccurs="0" />
      <!--cbc:AllowanceCharge-->
      <xs:element name="iskontoArtirim" type="iskontoArtirimTipi" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <!--AllowanceChargeType-->
  <xs:complexType name="iskontoArtirimTipi">
    <xs:sequence>
      <!--cbc:ChargeIndicator-->
      <xs:element name="artirim" type="xs:boolean" />
      <!--cbc:AllowanceChargeReason-->
      <xs:element name="neden" type="xs:string" minOccurs="0" />
      <!--cbc:MultiplierFactorNumeric-->
      <xs:element name="oran" type="xs:decimal" minOccurs="0" />
      <!--cbc:SequenceNumeric-->
      <xs:element name="siraNumarasi" type="xs:decimal" minOccurs="0" />
      <!--cbc:Amount-->
      <xs:element name="tutar" type="tutarTipi" />
      <!--cbc:BaseAmount-->
      <xs:element name="uygulandigiTutar" type="tutarTipi" minOccurs="0" />
      <!--cbc:PerUnitAmount-->
      <xs:element name="uygulananUrunMiktari" type="tutarTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--cac:PostalAddress-->
  <xs:complexType name="adresTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="sabitTanimlamaNumarasi" type="idTipi" minOccurs="0" />
      <!--cbc:PostalBox-->
      <xs:element name="postaKutusu" type="xs:string" minOccurs="0" />
      <!--cbc:Room-->
      <xs:element name="kapiNo" type="xs:string" minOccurs="0" />
      <!--cbc:StreetName-->
      <xs:element name="caddeSokak" type="xs:string" minOccurs="0" />
      <!--cbc:blockName-->
      <xs:element name="blok" type="xs:string" minOccurs="0" />
      <!--cbc:BuildingName-->
      <xs:element name="binaAdi" type="xs:string" minOccurs="0" />
      <!--cbc:BuildingNumber-->
      <xs:element name="binaNo" type="xs:string" minOccurs="0" />
      <!--cbc:CitySubdivisionName-->
      <xs:element name="ilce" type="xs:string" />
      <!--cbc:CityName-->
      <xs:element name="sehir" type="xs:string" />
      <!--cbc:PostalZone-->
      <xs:element name="postaKodu" type="xs:string" minOccurs="0" />
      <!--cbc:Region-->
      <xs:element name="kasabaKoy" type="xs:string" minOccurs="0" />
      <!--cbc:District-->
      <xs:element name="mahalle" type="xs:string" minOccurs="0" />
      <!--cac:Country-->
      <xs:element name="ulke" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <!--cac:PartyLegalEntity-->
  <xs:complexType name="sicilBilgileriTipi">
    <xs:sequence>
      <!--cbc:RegistrationName-->
      <xs:element name="kayitAdi" type="xs:string" minOccurs="0" />
      <!--cbc:CompanyID-->
      <xs:element name="kayitNumarasi" type="idTipi" minOccurs="0" />
      <!--cbc:RegistrationDate-->
      <xs:element name="kayitTarihi" type="tarihTipi" minOccurs="0" />
      <!--cbc:SoleProprietorshipIndicator-->
      <xs:element name="tekKisiyeAit" type="xs:boolean" minOccurs="0" />
      <!--cbc:CorporateStockAmount-->
      <xs:element name="odenmisSermaye" type="tutarTipi" minOccurs="0" />
      <!--cbc:FullyPaidSharesIndicator-->
      <xs:element name="halkaAcik" type="xs:boolean" minOccurs="0" />
      <!--cac:CorporateRegistrationScheme-->
      <xs:element name="kurumBilgileri" type="kurumsalSicilSemasiTipi" minOccurs="0" />
      <!--cac:HeadOfficeParty-->
      <xs:element name="merkezBilgisi" type="tarafBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--CorporateRegistrationSchemeType-->
  <xs:complexType name="kurumsalSicilSemasiTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="kayitYeriNumarasi" type="idTipi" minOccurs="0" />
      <!--cbc:Name-->
      <xs:element name="kayitYeriIsmi" type="xs:string" minOccurs="0" />
      <!--cbc:CorporateRegistrationTypeCode-->
      <xs:element name="kayitTipiKodu" type="xs:string" minOccurs="0" />
      <!--cac:JurisdictionRegionAddress-->
      <xs:element name="kayitYeriAdresi" type="adresTipi" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <!--ContactType-->
  <xs:complexType name="irtibatBilgileriTipi">
    <xs:sequence>
      <!--cbc:Telephone-->
      <xs:element name="tel" type="xs:string" minOccurs="0" />
      <!--cbc:Telefax-->
      <xs:element name="fax" type="xs:string" minOccurs="0" />
      <!--cbc:ElectronicMail-->
      <xs:element name="eposta" type="xs:string" minOccurs="0" />
      <!--cbc:Note-->
      <xs:element name="not" type="xs:string" minOccurs="0" />
      <!--cac:otherCommunication-->
      <xs:element name="digerIrtibatKanaliBilgisi" type="irtibatKanaliBilgisiTipi" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <!--CommunicationType-->
  <xs:complexType name="irtibatKanaliBilgisiTipi">
    <xs:sequence>
      <!--cbc:ChannelCode-->
      <xs:element name="kanalKodu" type="xs:string" />
      <!--cbc:Channel-->
      <xs:element name="kanalAdi" type="xs:string" minOccurs="0" />
      <!--cbc:Value-->
      <xs:element name="irtibatAdresi" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <!--PersonType-->
  <xs:complexType name="kisiBilgileriTipi">
    <xs:sequence>
      <!--cbc:FirstName-->
      <xs:element name="adi" type="xs:string" />
      <!--cbc:FamilyName-->
      <xs:element name="soyadi" type="xs:string" />
      <!--cbc:Title-->
      <xs:element name="unvan" type="xs:string" minOccurs="0" />
      <!--cbc:MiddleName-->
      <xs:element name="gobekAdi" type="xs:string" minOccurs="0" />
      <!--cbc:NameSuffix-->
      <xs:element name="adOnEki" type="xs:string" minOccurs="0" />
      <!--cbc:NationalityID-->
      <xs:element name="uyruk" type="idTipi" minOccurs="0" />
      <!--cac:FinancialAccount-->
      <xs:element name="hesapBilgisi" type="hesapBilgileriTipi" minOccurs="0" />
      <!--cac:IdentityDocumentReference-->
      <xs:element name="kimlikDokumani" type="dokumanBilgileriTipi" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--FinancialAccountType-->
  <xs:complexType name="hesapBilgileriTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="hesapNo" type="idTipi" />
      <!--cbc:CurrencyCode-->
      <xs:element name="hesapDovizBirimi" type="xs:string" minOccurs="0" />
      <!--cbc:PaymentNote-->
      <xs:element name="aciklama" type="xs:string" minOccurs="0" />
      <!--cac:FinancialInstitutionBranch/cbc:Name-->
      <xs:element name="sube" type="xs:string" minOccurs="0" />
      <!--cac:FinancialInstitutionBranch/cac:FinancialInstitution-->
      <xs:element name="banka" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--AttachmentType-->
  <xs:complexType name="ekliDosyaTipi">
    <xs:sequence>
      <!--cbc:EmbeddedDocumentBinaryObject-->
      <xs:element name="gomulu" type="gomuluEkliDosyaTipi" minOccurs="0" />
      <!--cac:ExternalReference-->
      <xs:element name="referans" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!--EmbeddedDocumentBinaryObjectType-->
  <xs:complexType name="gomuluEkliDosyaTipi">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <!--format-->
        <xs:attribute name="format" type="xs:string" use="optional" />
        <!--mimeCode-->
        <xs:attribute name="mimeType" type="xs:string" use="required" />
        <!--encodingCode-->
        <xs:attribute name="encodingCode" type="xs:string" use="optional" />
        <!--characterSetCode-->
        <xs:attribute name="charactersetCode" type="xs:string" use="optional" />
        <!--uri-->
        <xs:attribute name="uri" type="xs:string" use="optional" />
        <!--fileName-->
        <xs:attribute name="fileName" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <!--DeliveryType-->
  <xs:complexType name="gonderimSevkiyatTasimaBilgileriTipi">
    <xs:sequence>
      <!--cbc:ID-->
      <xs:element name="id" type="idTipi" minOccurs="0" />
      <!--cbc:Quantity-->
      <xs:element name="urunMiktari" type="miktarTipi" minOccurs="0" />
      <!--cbc:ActualDeliveryDate-->
      <xs:element name="gerceklesenTeslimTarihi" type="tarihTipi" minOccurs="0" />
      <!--cbc:ActualDeliveryTime-->
      <xs:element name="gerceklesenTeslimZamani" type="zamanTipi" minOccurs="0" />
      <!--cbc:LatestDeliveryDate-->
      <xs:element name="sonTeslimTarihi" type="tarihTipi" minOccurs="0" />
      <!--cbc:LatestDeliveryTime-->
      <xs:element name="sonTeslimZamani" type="zamanTipi" minOccurs="0" />
      <!--cbc:TrackingID-->
      <xs:element name="takipNumarasi" type="idTipi" minOccurs="0" />
      <!--cac:DeliveryAddress-->
